alias ll="ls -alF"
alias uuid=uuidgen
alias UUID="uuid | tr 'a-z' 'A-Z'"
alias guid="uuid | sed -e 's/-//g'"
alias GUID="guid | tr 'a-z' 'A-Z'"
alias ifmgit="git -c core.fileMode=false"
alias rpwd="sort -g -t : -k 3 /etc/passwd -o /etc/passwd"
alias upwd="sort -r -g -t : -k 3 /etc/passwd -o /etc/passwd"
alias suroot="rpwd && su root"
alias hass="cd /home/zhangle/ileler/bins/homeassistant && source ./bin/activate && ./start && tail -f ~/.homeassistant/home-assistant.log"
